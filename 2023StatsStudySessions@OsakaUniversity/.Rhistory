# サンプルデータフレームの作成
df <- data.frame(
city = c("東京", "大阪", "東京", "大阪", "東京"),
population = c(1000, 2000, 1500, 2500, 1300)
)
print(df)
df_grouped <- df %>%
group_by(city) %>%
summarise(avg_population = mean(population))
print(df_grouped)
